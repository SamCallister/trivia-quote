{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": "error"
    },
    "plugins": ["@typescript-eslint"]
}